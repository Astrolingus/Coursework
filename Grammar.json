{
    "<start>": "<stanza> % <stanza> % <stanza> % <couplet>",
    "<stanza>": "<first> % <mid> % <mid> % <last> % ",
    "<couplet>": "<first> % <last> % ",

    "<first>": "<clause> ",
    "<mid>": "<conjunction> <clause> | <clause> ",
    "<last>": "<clause>",

    "<clause>": "<subject> <predicate>",
    "<conjunction>":"and | or | because | then | so | while",

    "<subject>": "you | your <noun> ",
    "<predicate>": "<verb>",

    "<noun>":"heart | mind | body | soul",
    "<verb>":"bled | felt | dried | masturbated"
}
